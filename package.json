{
  "name": "rollup-plugin-lit-css",
  "version": "2.0.1",
  "description": "Rollup plugin to import css files as lit-element tagged css objects.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "nodemon -x 'npm test'",
    "test": "tape -r esm test/test.js | tap-summary"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bennypowers/rollup-plugin-lit-css.git"
  },
  "keywords": [
    "lit-element",
    "web-components",
    "css",
    "rollup"
  ],
  "eslint": {
    "env": {
      "node": true
    }
  },
  "esm": {
    "mode": "auto"
  },
  "author": "Benny Powers <web@bennypowers.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bennypowers/rollup-plugin-lit-css/issues"
  },
  "homepage": "https://github.com/bennypowers/rollup-plugin-lit-css#readme",
  "dependencies": {
    "rollup-pluginutils": "^2.8.2",
    "uglifycss": "0.0.29"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.0",
    "@types/uglifycss": "0.0.5",
    "cz-conventional-changelog": "^3.1.0",
    "esm": "^3.2.0",
    "lit-element": "^2.0.0",
    "nodemon": "^2.0.3",
    "rollup": "^1.1.2",
    "rollup-plugin-alias": "^2.2.0",
    "semantic-release": "^17.0.1",
    "tap-summary": "^4.0.0",
    "tape": "^4.9.2",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
